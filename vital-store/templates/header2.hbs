    <script type="text/javascript">
    	$(function(){
    		//navigation
    		//get current location
    		var locationNoQuery = window.location.href.split('?')[0];
    		//last chunk
    		var parts = locationNoQuery.split('/');
    		var lastPart = parts[parts.length - 1];
    		
    		$('.nav-aware a').each(function(index){
    			var a = $(this);
    			if(a.attr('href') == lastPart) {
    				a.addClass('active');
    			} else {
    				a.removeClass('active');
    			}
    		});
    		
    		//$('#header-dockbar').detach().insertAfter($('body > header'));
   		
    	});
    </script>
            
