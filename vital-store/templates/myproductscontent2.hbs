<div class="container">        
  <table class="table table-hover">
    <thead>
      <tr>
        <th>Index</th>
        <th>Product ID</th>
        <th>Product Name</th>
        <th>Terms</th>
        <th>Subscription Start Date</th>
        <th>Subscription End Date</th>
      </tr>
    </thead>
    <tbody>
      {{#each items}}<tr class="accordion-toggle"  data-toggle="collapse" data-target="#collapse{{number}}">
    <td>{{index}}</td>
    <td>{{id}}</td>
    <td>{{name}}</td>
    <td>{{terms}}</td>
    <td>{{startdate}}</td>
    <td>{{enddate}}</td>
</tr>
      
<tr>
    <td></td>
    <td colspan="6">
        <div id="collapse{{number}}" class="collapse in">
            Release Notes <br/>
            {{notes}} <br/>
        </div></td>
</tr>{{/each}}

    </tbody>
  </table>
</div>
<br><br><br><br><br>
